{
  "name": "Starter",
  "version": "0.0.1",
  "private": true,
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./app"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "rnstl && jest",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "^1.0.12",
    "react-navigation": "^3.0.8",
    "react-navigation-redux-helpers": "^2.0.8",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.1",
    "@storybook/addon-links": "^4.1.1",
    "@storybook/addon-storyshots": "^4.1.1",
    "@storybook/addons": "^4.1.1",
    "@storybook/react-native": "^4.1.1",
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "babel-runtime": "^6.26.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.5",
    "prop-types": "^15.6.2",
    "react-dom": "16.6.3",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg)$": "RelativeImageStub",
      ".+\\.(png)$": "jest-static-stubs/png"
    }
  }
}
